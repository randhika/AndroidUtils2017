<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <!-- 识别body -->
    <include
        layout="@layout/face_detection_body_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <!-- 识别底部工具 -->
    <include
        layout="@layout/detection_bottom_layout" />

   <io.silvrr.installment.facedetection.ui.view.FaceGuideStartView
        android:id="@+id/face_detection_guide_start"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <io.silvrr.installment.facedetection.ui.view.FaceGuideView
        android:id="@+id/face_guide_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"/>
</RelativeLayout>